<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>view/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BaseServlet</servlet-name>
        <servlet-class>controller.BaseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EncodeIGA</servlet-name>
        <servlet-class>controller.EncodeIGA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewIGA</servlet-name>
        <servlet-class>controller.ViewIGA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewProgramList</servlet-name>
        <servlet-class>controller.ViewProgramList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCurriculumList</servlet-name>
        <servlet-class>controller.ViewCurriculumList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCourseList</servlet-name>
        <servlet-class>controller.ViewCourseList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCreateProgram</servlet-name>
        <servlet-class>controller.ViewCreateProgram</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCreateCurriculum</servlet-name>
        <servlet-class>controller.ViewCreateCurriculum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCreateCourse</servlet-name>
        <servlet-class>controller.ViewCreateCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewSearchPA</servlet-name>
        <servlet-class>controller.ViewSearchPA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewSearchPO</servlet-name>
        <servlet-class>controller.ViewSearchPO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewSearchPI</servlet-name>
        <servlet-class>controller.ViewSearchPI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BaseServlet</servlet-name>
        <url-pattern>/BaseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EncodeIGA</servlet-name>
        <url-pattern>/EncodeIGA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewIGA</servlet-name>
        <url-pattern>/ViewIGA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewProgramList</servlet-name>
        <url-pattern>/ViewProgramList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCurriculumList</servlet-name>
        <url-pattern>/ViewCurriculumList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCourseList</servlet-name>
        <url-pattern>/ViewCourseList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCreateProgram</servlet-name>
        <url-pattern>/ViewCreateProgram</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCreateCurriculum</servlet-name>
        <url-pattern>/ViewCreateCurriculum</url-pattern>
    </servlet-mapping>
