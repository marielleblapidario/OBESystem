        <url-pattern>/ViewSearchPO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewSearchPI</servlet-name>
        <url-pattern>/ViewSearchPI</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
